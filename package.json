  "private": true,
  "scripts": {
    "format": "prettier --write --loglevel=warn 'src/**/*.js'",
    "start": "node_modules/react-native/packager/packager.sh",
    "xcode": "open ios/example.xcodeproj",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "ios:dev": "NODE_ENV=development react-native run-ios",
    "android:dev": "NODE_ENV=development react-native run-android",
    "test": "jest",
    "patch": "./patch.sh",
    "testBuild:e2e": "detox build --configuration ios.sim.debug",
    "test:e2e": "detox test --configuration ios.sim.debug --cleanup",
    "testBuild:e2e:android": "detox build --configuration android.emu.debug",
    "test:e2e:android": "detox test --configuration android.emu.debug --cleanup",
    "fakeRest": "./node_modules/json-server/bin/index.js --watch country.json",
    "menu": "adb -s 4NTNW17410000733 shell input keyevent 82",
    "refresh": "adb -s 4NTNW17410000733 shell input keyevent 82 && adb shell input tap 100 280",
    "remote": "adb -s 4NTNW17410000733 shell input keyevent 82 && adb shell input tap 100 341",
    "debugger": "npm run rndebugger && npm run remote",
    "rndebugger": "open \"rndebugger://set-debugger-loc?host=192.168.0.226&port=8081\"",
    "restart": "react-native start --reset-cache",
    "androidBuildFix": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --assets-dest ./android/app/src/main/res/",
    "iosBuildFix": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ./ios/main.jsbundle --assets-dest ./ios/example/Images.xcassets/",
    "changelog": "gitchangelog > change.log",
    "json-server": "json-server ./mockdata/index.js  --port 3000 ",
    "checkApk": "jarsigner -verify ./android/app/release/app-release.apk",
    "installUsbApk": "adb -s C1NGA1E7B1305312 install ./android/app/release/app-release.apk",
    "installUsbApk2": "adb -s 4NTNW17410000733 install ./android/app/release/app-release.apk",
    "copyApkDeploy": "cp ./android/app/release/app-release.apk ./deploy/ElephantApp/app-1.0.2-build-1.apk",
    "signApk": "./android/gradlew assembleRelease -Pandroid.injected.signing.store.file=./android/app/release.keystore -Pandroid.injected.signing.store.password=android -Pandroid.injected.signing.key.alias=release.keystore -Pandroid.injected.signing.key.password=android --debug",
    "deployApk": "npm run signApk && npm run copyApkDeploy && npm run checkApk",
    "androidBundle": "react-native bundle --platform android --entry-file index.js --bundle-output ./android-bundle/index.android.bundle --assets-dest ./android-bundle --dev false",
    "androidUpdate": "code-push release ElephantApp-android ./android-bundle/ 1.0.2 --d Production --description 'new: 更新aaa' --mandatory true",
    "iosBundle": "react-native bundle --platform ios --entry-file index.js --bundle-output ./ios-bundle/main.jsbundle --assets-dest ./ios-bundle --dev false",
    "iosUpdate": "code-push release ElephantApp-ios ./ios-bundle/ 1.0.2 --d Production --description 'new：测试' --mandatory true",
    "iosUpdateDev": "code-push release ElephantApp-ios ./ios-bundle/ 0.2.0 --description 'new：7-30 更新界面ui修改' --mandatory true",
    "copyFromExample": "mkdir ./ios && cp -rf ./ios-example/* ./ios/ && sleep 5 && mkdir ./android && cp -rf ./android-example/* ./android/ && npm run syncKeystore",
    "syncKeystore": "cp ./deploy/release.properties ./android/ && cp ./deploy/debug.properties ./android/ && cp ./deploy/release.keystore ./android/app/ && cp ./deploy/debug.keystore ./android/app/",
    "clearDeploy": "code-push deployment clear ElephantApp-ios Production && code-push deployment clear ElephantApp-android Production",
    "syncAppVersion": "./syncAppVersion.sh 20180806.2.0",
    "test:unit": "jest src/ --maxWorkers=2",
    "reinstall:ios-sim": "xcrun simctl uninstall booted 'com.shandudata.zhimekaimen' && xcrun simctl install booted ios/build/Build/Products/Debug-iphonesimulator/example.app && xcrun simctl launch booted 'com.shandudata.zhimekaimen'",
    "uninstall:ios-sim": "xcrun simctl uninstall booted 'com.shandudata.zhimekaimen'",
    "install:ios-sim": "xcrun simctl install booted /Users/ghost/Library/Developer/Xcode/DerivedData/example-geepeoudotduvrbourwibmclytxq/Build/Products/Debug-iphonesimulator/example.app",
    "boot:ios-sim": "xcrun simctl launch booted 'com.shandudata.zhimekaimen'",
    "h5": "cd ./h5 && npm run publish"
  },
